node {
   stage ('Scm Checkout'){
      git 'https://github.com/pampanoor/my-app'
   }
   stage('Compile-Package'){
   def mvnhome="/usr/share/maven"
       sh "${mvnhome}/bin/mvn clean package"
   }
   stage('SonarQube Analysis'){
   def mvnhome="/usr/share/maven"
	   withSonarQubeEnv('sonar-1'){
			sh "${mvnhome}/bin/mvn sonar:sonar"
	   }
   }
}
